                            <input type="text" class="form-control" id="chat-input" placeholder="Nhập tin nhắn của bạn..." autocomplete="off">
                        </div>
                        <div class="col-auto">
                            <div class="chat-input-links ms-2">
                                <div class="d-flex gap-4 align-items-center">
                                    <a href="#!" class="link text-body fs-4" data-bs-toggle="tooltip" data-bs-html="true" aria-label="Đính kèm liên kết" data-bs-original-title="Đính kèm liên kết">
                                        <i class="ri-link-m"></i>
                                    </a>
                                    <div class="file-upload">
                                        <a href="#!" class="link text-body fs-4" data-bs-toggle="tooltip" data-bs-html="true" title="Thêm tệp">
                                            <input class="file-upload-item" type="file">
                                            <i class="ri-file-add-line"></i>
                                        </a>
                                    </div>
                                    <a href="#!" class="link text-body fs-4" data-bs-toggle="tooltip" data-bs-html="true" title="Gửi biểu tượng cảm xúc">
                                        <i class="ri-emoji-sticker-line"></i>
                                    </a>
                                </div>
                            </div>
                        </div>
                        <div class="col-auto">
                            <div class="ms-2">
                                <div class="links-list-item">
                                    <button type="submit" class="btn btn-light" aria-label="Gửi"><i class="bi bi-send-fill"></i></button>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <div class="app-chat-overlay" id="appChatOverlay"></div>
</div>

